{
  "name": "comite-aereo",
  "version": "1.0.0",
  "description": "Módulo de usuarios",
  "main": "src/application/index.js",
  "scripts": {
    "start-dev": "DEBUG=app:* nodemon src/application/server",
    "dev": "DEBUG=app:* nodemon src/application/server",
    "dev-secure": "NODE_ENV=secure DEBUG=app:* nodemon src/application/server",
    "startdev": "DEBUG=app:* nodemon src/application/server",
    "test": "npm run setup && npm run seeders && npm run test-db && npm run test-domain",
    "test-db": "ava src/infrastructure/tests/ --verbose",
    "test-domain": "ava src/domain/tests/ --verbose",
    "test-common": "DEBUG=app:* ava src/common/tests/ --verbose",
    "setup": "DEBUG=app:* node src/infrastructure/setup.js",
    "seeders": "sequelize db:seed:all --seeders-path src/infrastructure/seeders --config src/infrastructure/lib/config-seeders.js && npm run migrar",
    "seeders-update": "NODE_ENV=production sequelize db:seed:all --seeders-path src/infrastructure/seeders/update --config src/lib/config-seeders.js",
    "seeders-prod": "NODE_ENV=production npm run seeders",
    "sincronizar": "node cronjob/sincronizacion",
    "sincronizarAASANA": "node cronjob/sincronizacionVuelosConAASANA",
    "migrar": "sequelize db:migrate --migrations-path src/infrastructure/migrations --config src/infrastructure/lib/config-seeders.js",
    "lint": "semistandard"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.agetic.gob.bo:base/base-usuarios.git"
  },
  "keywords": [
    "Módulo",
    "usuario"
  ],
  "author": "Omar Gutiérrez Condori <ogutierrez@agetic.gob.bo>",
  "license": "MIT",
  "dependencies": {
    "app-iop": "git+ssh://git@gitlab.agetic.gob.bo:base/app-iop.git",
    "app-logs": "^1.1.5",
    "app-mopsv": "git+ssh://git@gitlab.agetic.gob.bo:aereo/app-mopsv.git",
    "app-notificaciones": "git+ssh://git@gitlab.agetic.gob.bo:agetic/app-notificaciones.git",
    "app-params": "^0.2.5",
    "body-parser": "^1.18.2",
    "casual": "^1.5.19",
    "chalk": "^2.4.1",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "csvtojson": "^1.1.12",
    "defaults": "^1.0.3",
    "express": "^4.16.3",
    "express-asyncify": "^1.0.0",
    "express-fileupload": "^0.4.0",
    "express-jwt": "^5.3.1",
    "express-jwt-permissions": "^1.2.0",
    "graphql": "^0.13.2",
    "graphql-server-express": "^1.3.6",
    "graphql-tools": "^3.0.1",
    "inquirer": "^5.1.0",
    "intl": "^1.2.5",
    "intl-messageformat": "^2.2.0",
    "jsonwebtoken": "^8.2.0",
    "minimist": "^1.2.0",
    "moment": "^2.22.1",
    "node-jose": "^1.0.0",
    "nodemailer": "^4.5.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "nodemon": "^1.17.5",
    "openid-client": "^2.4.4",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.33.4",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "semistandard": "^12.0.0"
  }
}
